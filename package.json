{
  "name": "jsrouter",
  "version": "1.0.0",
  "description": "Minimal client side router",
  "main": "dist/jsrouter.min.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "lint-src": "./node_modules/.bin/eslint ./src",
    "lint-test": "./node_modules/.bin/eslint ./test",
    "lint": "npm run lint-src && npm run lint-test",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src $(npm bin)/mocha -R dot --compilers js:babel-core/register --require ./test/setup.js --recursive",
    "clean": "rm -rf dist/*",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/jsrouter.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/jsrouter/issues"
  },
  "keywords": [
    "router",
    "route",
    "routes",
    "routing",
    "route-recognizer",
    "javascript"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "chai": "3.3.0",
    "eslint": "1.5.1",
    "eslint-config-airbnb": "0.0.9",
    "eslint-plugin-react": "3.4.2",
    "istanbul": "0.3.21",
    "istanbul-instrumenter-loader": "0.1.3",
    "jsdom": "8.3.1",
    "mocha": "2.3.3",
    "node-libs-browser": "0.5.3",
    "sinon": "1.17.3",
    "webpack": "1.12.2"
  },
  "dependencies": {
    "route-recognizer": "0.1.9"
  }
}
